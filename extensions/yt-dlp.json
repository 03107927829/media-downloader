{
    "BackendPath": "${default}",
    "BatchFileArgument": "-a",
    "CanDownloadPlaylist": true,
    "CommandName": "yt-dlp",
    "CommandName32BitWindows": "yt-dlp_x86.exe",
    "CommandNameWindows": "yt-dlp.exe",
    "ControlJsonStructure": {
        "Connector": "&&",
        "lhs": {
            "startsWith": "[download]"
        },
        "rhs": {
            "contains": "ETA"
        }
    },
    "CookieArgument": "--cookies",
    "DefaultDownLoadCmdOptions": [
        "--newline",
        "--ignore-config",
        "--no-playlist",
        "-o",
        "%(title)s-%(id)s.%(ext)s"
    ],
    "DefaultListCmdOptions": [
        "--print",
        "%(formats)j"
    ],
    "DownloadUrl": "https://api.github.com/repos/yt-dlp/yt-dlp/releases/latest",
    "LikeYoutubeDl": true,
    "Name": "yt-dlp",
    "OptionsArgument": "-f",
    "PlayListIdArguments": [
        "--get-id",
        "--get-title",
        "--get-thumbnail"
    ],
    "PlaylistItemsArgument": "--playlist-items",
    "RemoveText": [
    ],
    "ReplaceOutputWithProgressReport": false,
    "RequiredMinimumVersionOfMediaDownloader": "",
    "ShowListTableBoundary": {
        "ColumnNumber": "0",
        "Comparator": "contains",
        "String": "--"
    },
    "ShowListTableExtraBoundaries": [
        {
            "ColumnNumber": "0",
            "Comparator": "equals",
            "String": "format"
        },
        {
            "ColumnNumber": "0",
            "Comparator": "contains",
            "String": "--"
        },
        {
            "ColumnNumber": "0",
            "Comparator": "equals",
            "String": "ID"
        }
    ],
    "SkipLineWithText": [
        "(pass -k to keep)"
    ],
    "SplitLinesBy": [
        "\n"
    ],
    "VersionArgument": "--version",
    "VersionStringLine": 0,
    "VersionStringPosition": 0
}
